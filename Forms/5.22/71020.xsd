									<xs:attribute name="fact" type="xs:string"/>
									<xs:attribute name="fact_proc" type="xs:string"/>
									<xs:attribute name="plan_to_1" type="xs:string"/>
									<xs:attribute name="plan_proc_to_1" type="xs:string"/>
