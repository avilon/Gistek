									<xs:sequence>
										<xs:element name="child" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="id" use="required" type="xs:byte"/>
												<xs:attribute name="measure" use="required" type="xs:string"/>
												<xs:attribute name="code" use="required" type="xs:byte"/>
												<xs:attribute name="plan_downto_2" use="required" type="xs:string"/>
												<xs:attribute name="fact_downto_2" use="required" type="xs:string"/>
												<xs:attribute name="plan_downto_1" use="required" type="xs:string"/>
												<xs:attribute name="plan_proc_downto_1" use="required" type="xs:string"/>
												<xs:attribute name="fact_downto_1" use="required" type="xs:string"/>
												<xs:attribute name="fact_proc_downt_1" use="required" type="xs:string"/>
												<xs:attribute name="plan" use="required" type="xs:string"/>
												<xs:attribute name="plan_proc" use="required" type="xs:string"/>
												<xs:attribute name="fact" use="required" type="xs:string"/>
												<xs:attribute name="fact_proc" use="required" type="xs:string"/>
												<xs:attribute name="plan_to_1" use="required" type="xs:string"/>
												<xs:attribute name="plan_proc_to_1" use="required" type="xs:string"/>
												<xs:attribute name="fact_to_1" use="required" type="xs:string"/>
												<xs:attribute name="fact_proc_to_1" use="required" type="xs:string"/>
												<xs:attribute name="plan_to_2" use="required" type="xs:string"/>
												<xs:attribute name="plan_proc_to_2" use="required" type="xs:string"/>
												<xs:attribute name="fact_to_2" use="required" type="xs:string"/>
												<xs:attribute name="fact_proc_to_2" use="required" type="xs:string"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="name" use="required" type="xs:string"/>
									<xs:attribute name="code" type="xs:byte"/>
									<xs:attribute name="division" use="required" type="xs:byte"/>
									<xs:attribute name="measure" type="xs:string"/>
									<xs:attribute name="plan_downto_2" type="xs:string"/>
									<xs:attribute name="fact_downto_2" type="xs:string"/>
